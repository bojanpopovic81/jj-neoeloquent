{
    "name": "jelena-dragojlovic/jj-neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": ["laravel", "neo4j", "graph", "database", "neoeloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.3.*",
        "illuminate/database": "5.3.*",
        "illuminate/pagination": "5.3.*",
        "heydavid713/neo4jphp": "0.1.*",
        "nesbot/carbon": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jelena-dragojlovic/jj-neoeloquent",
                "version": "master",
                "source": {
                    "url": "git://github.com/jelena-dragojlovic/jj-neoeloquent.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0" : {
                        "jelena-dragojlovic/jj-neoeloquent" : "src"
                    }
                }
            }
        }
    ],
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-0": {
            "jelena-dragojlovic\\jj-neoeloquent": "src/"
        }
    },
    "minimum-stability": "stable"
}
